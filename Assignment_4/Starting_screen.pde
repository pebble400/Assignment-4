class Start {
  PVector Pos = new PVector (0,0);
  float Width = 0;
  float Height = 0;
  color Colour;
  String Text;
  Boolean Pressed = false;
  Boolean Clicked = false;
  
  Start(int x, int y, int w, int h, String t, int r, int g, int b) {
    
    Pos.x = x;
    Pos.y = y;
    Width = w;
    Height = h;
    Colour = color(r,g,b);
    Text = t;
  }
  
  
  
  
  
  
  
  
  
  
  
  
  
}
